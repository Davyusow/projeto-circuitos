        </entry>
        <entry>
          <string>Code</string>
          <string>module ULA(
	input [31:0] operand1, operand2,
	input [3:0] ALU_control_vector,
	output [31:0] ALU_result,
	output reg zero
);

	reg [31:0] ALU_result;

	always @(*) begin
           case (ALU_control_vector)
              0: ALU_result = operand1 &amp; operand2;
              1: ALU_result = operand1 | operand2;
              2: ALU_result = operand1 + operand2;
              6: ALU_result = operand1 - operand2;
	      7: ALU_result = operand1 &lt; operand2;
              12: ALU_result = ~(operand1 | operand2);
	      default: ALU_result = 32&apos;d0;
	   endcase
	   if(ALU_result == 32&apos;d0)begin
           	zero = 1&apos;b1;
           end else begin
	   	zero = 1&apos;b0;
           end
        end

endmodule</string>
        </entry>
        <entry>
          <string>externalOutputs</string>
          <string>ALU_result:32,zero</string>
        </entry>
        <entry>
