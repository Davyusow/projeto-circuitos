   let a = random(1&lt;&lt;8);
   let b = random(1&lt;&lt;8);
   (a) (b) 0 (a &amp; b)
   (a) (b) 1 (a | b)
   (a) (b) 2 (a + b)
   (a) (b) 3 (a - b)
 end loop</dataString>
